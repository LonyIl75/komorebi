generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Bouchers {
  id String  @id @default(auto()) @map("_id") @db.ObjectId
  date Int
  ProductImage_alt String?
  ProductImage_src String?
  ProductPrice String
  ProductPricePerWeight String  
  ProductShopName String
  ProductShopName_href String?
  ProductTitle String
  ProductTitle_href String?
  ProductType String @unique
  ProductWeight String
}